<%= form_for(@user) do |f| %>
  <%= f.error_notification %>
  <div class="form_inputs">
  <%=f.input :email %>
  <%=f.input :password %>
  <%=f.input :address %>
  <%=f.input :name %>
  <%=f.input :last_name %>
  <%=f.input :email %>
  <%if current_user.admin? %>
  <%= f.input :admin %>
  <%end>

  </div> 
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
